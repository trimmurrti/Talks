import Foundation

%{
types = [
    ('String', '""'),
    ('Array', '[]'),
    ('Set', 'Set()')
]
}%

% for type, default_value in types
extension ${type}: Emptiable {

    static var `default`: ${type} {
        return ${default_value}
    }
}

% end
